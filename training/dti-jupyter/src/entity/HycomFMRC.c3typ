/**
HycomFMRC.c3typ
A collection of Hycom Forcast Model Runs
*/
entity type HycomFMRC mixes File schema name HC_FMRC {
    //
    dataset: !string
    //
    run: !datetime
    // Note it could be the case that lat_min etc are always the same for a given dataset
    // So perhaps we should have a HycomDataset type that contains these fields.
    lat_min: !double
    //
    lat_max: !double
    //
    lon_min: !double
    //
    lon_max: !double

}