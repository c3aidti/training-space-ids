/**
FRMCFile.c3typ
Files comprising a {@link FMRCDataArchive}.
*/
entity type FMRCFile schema name "FRMC_FL" {
    // The {@link HycomFRMC} that these files belong to.
    //dataArchive: !FMRCDataArchive
    fmrc: !HycomFMRC
    //  FRMC file name
    fileName: string post default 'id'
    // Time coverage for this file
    timeCoverage: !TimeRange
    // Time stride for this file
    timeStride: !int
    // TimeStride units for this file
    timeStrideUOM: Unit post default "{id:'hour'}"
    // the varaibles contained in this file    
    vars: !string
    //  FRMC file type
    fileType: !string enum('netcdf4','netcdf')
    // Status of the data download
    status: string enum('downloaded','not_downloaded','downloading','error') post default '"not_downloaded"'
    // The {@link File} object for this file
    file: File
    // Download a file from the Hycom server and save to the C3 file system
    @dependency(include = "this,fmrc.subsetOptions,fmrc.urlPath")
    @py(env=hycom_1_0_0)
    download: member function(extDir: !string): File
}