/**
FRMCFile.c3typ
Files comprising a {@link FMRCDataArchive}.
*/
entity type FMRCFile mixes File schema name "FRMC_FL" {
    // The {@link HycomFRMC} that these files belong to.
    dataArchive: !FMRCDataArchive
    //  FRMC file name
    name: !string
    // Time coverage for this file
    timeCoverage: !TimeRange
    // Time stride for this file
    timeStride: !int
    // TimeStride units for this file
    timeStrideUOM: Unit post default "{id:'hour'}"
    // Geospatial coverage for this file
    geospatialCoverage: !GeospatialCoverage
    // Geospatial resolution for this file
    geospatialResolution: !GeospatialResolution post default 'dataArchive.fmrc.dataset.geospatialResolution'
    // the varaibles contained in this file    
    vars: !string
    //  FRMC file type
    fileType: !string enum('netcdf4','netcdf')
    // Url used to download the file
    thredds_url: !string
    // Theownload options
    // Status of the data download
    status: !string enum('downloaded','not_downloaded','downloading','error')
}