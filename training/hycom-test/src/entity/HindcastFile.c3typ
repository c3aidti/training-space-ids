/**
HindcastFile.c3typ
*/
entity type HindcastFile schema name "HNDCST_FL" {
    // The {@link HycomHindcast} that these files belong to.
    hindcastArchive: !HindcastArchive
    //
    subsetOptions: !HycomSubsetOptions
    //  FRMC file name
    //fileName: string post default 'id'
    // Time coverage for this file
    //timeCoverage: !TimeRange
    // Time stride for this file
    //timeStride: !int
    // TimeStride units for this file
    //timeStrideUOM: Unit post default "{id:'hour'}"
    // the varaibles contained in this file    
    //vars: !string
