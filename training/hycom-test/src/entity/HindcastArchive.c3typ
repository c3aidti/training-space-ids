/**

*/
entity type HindcastArchive schema name 'HNDCST_ARCHV' {
    //
    dataset: !HycomDataset
    //
    name: !string
    //
    description: !string
    //
    urlPath: !string
    //
    timeCoverage: TimeRange
    //
    geospatialCoverage: GeospatialCoverage post default 'dataset.geospatialCoverage'
    // Utility method to download a file from the archive
    downloadLocal: function(hycomSubsetOptions: !HycomSubsetOptions, localDir: string): string py server 

}