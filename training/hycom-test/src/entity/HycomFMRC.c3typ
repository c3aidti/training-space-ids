/**
HycomFMRC.c3typ
A collection of Hycom Forcast Model Runs
*/
entity type HycomFMRC mixes File schema name "HC_FMRC" {
    // The name of the {@link HycomDataset} that this FMRC is based on
    dataset: HycomDataset
    // Date of the forecast run
    run: !string
    // Start of time coverage
    start: !datetime
    // End of time coverage
    end: !datetime
    // URL of the forecast run on the server
    thredds_url: !string
    // Anticipated expiration date for this run's data on Thredds server
    expires: !datetime
    // Indicates whether this run is still active
    is_expired: !boolean
}