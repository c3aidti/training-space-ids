/**
FMRCDataArchive.c3typ
A Data archive for a {@link HycomFMRC} containing associated {@link FMRCFile}s.
*/
entity type FMRCDataArchive schema name 'FMRC_DT_ARCHV' {
    // The {@link HycomFMRC} associated with this archive.
    fmrc: !HycomFMRC
    // The {@link FMRCSubsetOptions} options for this archive.
    subsetOptions: !FMRCSubsetOptions
    // The {@link FMRCDownloadOptions} for how to retrive the files that make up the dataset
    downloadOptions: !FMRCDownloadOptions
    // Collection of {@link FMRCFile}s associated with this archive.
    dataFiles: [FMRCFile](dataArchive,id)
    // Whether {@link FMRCFile}s have been staged for download.
    // Commented out due to unknown "extraneous property" error
    //staged: boolean post default false
    // Total size of the {@link FMRCFile}s in bytes.
    //archiveSize: integer stored calc 'sum(dataFiles.file.contentLength)'
    // Method to stage {@link FMRCFile} records for download from {@link HycomFMRC}s.
    @py(env='hycom_1_0_0')
    stageFMRCFiles: member function(): [FMRCFile]
}