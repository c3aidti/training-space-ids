/**
* GaussianProcessRegressionTechnique.c3typ
* Defines hyper parameters for GP Regression.
*/
entity type GaussianProcessRegressionTechnique mixes MLTechnique type key 'GPRTCHNQ' {
    // random seed
    @ML(hyperParameter=true)
    randomState: integer
    // the kernel object
    @ML(hyperParameter=true)
    kernel: SklearnGPRKernel schema suffix "KRNL"
}