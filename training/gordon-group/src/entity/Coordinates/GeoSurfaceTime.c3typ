/**
* GeoSurfaceTime.c3typ
* A space-time point where space is the Earth's surface.
*/
@db(persistDuplicates=false,
    compactType=true,
    shortId=true)
entity type GeoSurfaceTime schema name 'GSRFC_TM' {
    // the latitude
    latitude: !float
    // the longitude
    longitude: !float
    // the timestamp
    time: !datetime
}