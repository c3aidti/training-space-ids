/**
* TransformSourceUtilityGridToUtilityGrid.c3typ
* Transforms data from {@link SourceUtilityGrid} to the {@link UtilityGrid} type
*/
type TransformSourceUtilityGridToUtilityGrid mixes UtilityGrid transforms SourceUtilityGrid {
    // The id of the battery
    id: ~ expression "id"

    // Minimum rate of {@link UtilityGrid}
    rateMin: ~ expression "rateMin"
    
    // Maximum rate of {@link UtilityGrid}
    rateMax: ~ expression "rateMax"
}